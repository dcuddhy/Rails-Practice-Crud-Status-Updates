<%= notice %>

<% if @update.errors.present? %>
    <% @update.errors.full_messages.each do |message| %>
      <%= message %>
    <% end %>
<% end %>

<%= form_for @update do |f| %>

  <%= f.label :status %>
  <%= f.text_field :status %>

  <%= f.label :user %>
  <%= f.text_field :user %>

  <%= f.label :likes %>
  <%= f.number_field :likes %>

  <%= f.submit %>

  <% end %>
